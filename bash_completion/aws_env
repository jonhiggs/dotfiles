_aws_env_completion() {
    local command="$1"
    local word="$2"
    local options=$(yak --list-roles)
    if [[ -f ${HOME}/.aws/credentials ]]; then
      local options+=$(grep -e '^\[.*\]$' ${HOME}/.aws/credentials | tr -d '[]')
    fi
    COMPREPLY=($(compgen -W "${options}" -- ${word}))
    return 0
}

complete -F _aws_env_completion aws_env
