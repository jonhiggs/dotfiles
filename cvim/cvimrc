let configpath = '/Users/jonhiggs/etc/dotfiles/cvim/cvimrc'
set localconfig

set smoothscroll
let barposition = "bottom"

unmap <C-w>
unmap <C-n>
unmap <C-p>

map <C-d> scrollPageDown
map <C-u> scrollPageUp

imap <C-a> beginningOfLine
imap <C-e> endOfLine
imap <C-w> deleteWord
imap <M-d> deleteForwardWord

map <C-n> tabNext
map <C-p> tabPrevious
