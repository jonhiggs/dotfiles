source ${HOME}/etc/dotfiles/bash/src/bash-preexec/bash-preexec.sh
unset preexec_functions
unset precmd_functions

for file in ${HOME}/etc/dotfiles/bash/include.d/preexec_functions.d/*; do
  [[ -f ${file} ]] && source ${file}
done

for file in ${HOME}/etc/dotfiles/bash/include.d/precmd_functions.d/*; do
  [[ -f ${file} ]] && source ${file}
done

