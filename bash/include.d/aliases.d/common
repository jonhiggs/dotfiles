alias cdd='cd ../; ls'
alias grpe="grep"
alias pwgen="pwgen -s 14 1"

alias find="echo 'use fd' >&2"

alias kctl="kubectl"

alias dict="cat ${HOME}/etc/vimfiles/data/site/spell/custom-dictionary.utf-8.add ${HOME}/etc/vimfiles/data/site/spell/en_au-words.txt | sort -f"
alias weather="redis-cli get weather_data | jq"

hash -p ${HOME}/etc/dotfiles/bash/src/bash-my-aws/bin/bma bma
hash -p ${HOME}/src/kumo/bin/kumo kumo

alias t="${HOME}/bin/tig_selector"

