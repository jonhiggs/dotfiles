Term.termName: xterm-256color
xterm*background: black
xterm*foreground: lightgray
xterm*faceName:Terminus:style=Regular:size=10:antialias=false

! Fonts
URxvt.font:        -xos4-terminus-medium-*-*-*-18-*-*-*-*-*-*-*
URxvt.boldFont:    -xos4-terminus-bold-*-*-*-18-*-*-*-*-*-*-*
URxvt.letterSpace: -1
URxvt.scrollBar: false

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! rofi
rofi.bw:                            2
rofi.case-sensitive:                false
rofi.columns:                       1
rofi.eh:                            1
rofi.font:                          Source Code Pro 10
rofi.fuzzy:                         true
rofi.glob:                          false
rofi.hide-scrollbar:                true
rofi.kb-mode-next:                  Tab
rofi.kb-row-tab:
rofi.levenshtein-sort:              true
rofi.line-margin:                   0
rofi.line-padding:                  0
rofi.lines:                         8
rofi.location:                      0
rofi.m:                             -1
rofi.modi:                          window,run
rofi.opacity:                       75
rofi.padding:                       0
rofi.parse-hosts:                   false
rofi.parse-known-hosts:             true
rofi.separator-style:               solid
rofi.sidebar-mode:                  false
rofi.theme:                         Arc-Dark
rofi.tokenize:                      true
rofi.width:                         20

xclock*background:                  black
xclock*foreground:                  dimgrey
xclock*analog:                      false
xclock*font:                        -xos4-terminus-medium-*-*-*-14-*-*-*-*-*-iso10646-1
xclock*render:                      false
xclock*strftime:                    %m/%d %H:%M:%S
xclock*update:                      1

