# SOURCE IN RVM
[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"

# SETUP SSH AGENT
if [[ -e ~/.ssh/id_rsa ]]; then
  [[ -z ${SSH_AGENT_PID} ]] && eval $(ssh-agent)
fi

# BASH COMPLETION
[[ -f /usr/local/etc/bash_completion ]] && . /usr/local/etc/bash_completion

# AWS COMPLETION
complete -C aws_completer aws

